 <% @microposts.each do |micropost| %>
  <div class="diary-container">
        <div class="diary-entries">
            <ul id="entriesList" class="list-group">
          <p>
            <%= micropost.content %>
            <%= micropost.created_at.strftime("%Y-%m-%d") %>
            <%= button_to "🗑️", micropost_path(slug: @user.slug, id: micropost.id), method: :delete %> 
          </p>
          </ul>
          </div>
  </div>
    
<% end %>


